{
  "manifest_version": 2,

  "name": "Cookie Cracker",

  "description": "Crack the cookie clicker",
  "version": "0.2",

  "content_scripts": [
    {
      "matches": ["https://orteil.dashnet.org/cookieclicker/"],
      "js": ["game.js"]
    }
  ],

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "Cookieclicker-grandmacookie.png",
    "default_title": "Cookie Cracker!"
  }
}
